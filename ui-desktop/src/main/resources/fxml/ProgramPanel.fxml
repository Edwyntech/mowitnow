<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="programPanel" prefWidth="200.0" type="VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">

   <children>
      <TextArea fx:id="programTextArea" styleClass="program-text-area" VBox.vgrow="ALWAYS" />
      <Label fx:id="errorMessagesLabel" styleClass="error-messages-label" visible="false" wrapText="true" />
   </children>

   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
